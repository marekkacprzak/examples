export VERSION=1.20
export OS=xUbuntu_20.04
export ROOT=https://download.opensuse.org/repositories
export REPO=$ROOT/devel:/kubic:/libcontainers:/stable
VERSION="v1.32.0"
export CRICTL_URL=https://github.com/kubernetes-sigs/cri-tools/releases/download/$VERSION/crictl-$VERSION-linux-amd64.tar.gz

export WHEREDOWNLOAD=https://github.com/kubernetes-sigs/cri-tools

sudo tar zxvf crictl-$VERSION-linux-amd64.tar.gz -C /usr/local/bin
rm -f crictl-$VERSION-linux-amd64.tar.gz
